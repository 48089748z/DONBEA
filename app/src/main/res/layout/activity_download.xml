<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.extractor.uri48089748z.converter.DownloadActivity"
    android:background="@color/GREY_BACKGROUND_DARK_SUPER"
    android:weightSum="100"
    android:id="@+id/LLdownload">
    <TextView
        android:id="@+id/TVsplitbar8"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_weight="0.1"
        android:background="@color/GREY_TEXT_LIGHT_SUPER">
    </TextView>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_weight="3"/>
    <TextView
        android:id="@+id/TVheaderDownload"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_weight="10"
        android:text="@string/converting_video"
        android:textColor="@color/GREY_TEXT_LIGHT_SUPER"
        android:gravity="center"
        android:textSize="32sp"
        android:textStyle="bold"/>

    <ImageView
        android:id="@+id/IVloadingDownload"
        style="?android:attr/progressBarStyleLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@mipmap/ic_logo"
        android:contentDescription="@string/logo" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_weight="2"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="100"
        android:orientation="horizontal">
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="10"/>

        <TextView
            android:id="@+id/TVtitleDownload"
            android:layout_width="1dp"
            android:layout_height="wrap_content"
            android:layout_weight="80"
            android:textSize="20sp"
            android:gravity="center"
            android:textStyle="bold"
            android:textColor="@color/GREY_TEXT_LIGHT_SUPER"
            android:text="@string/please_wait"/>
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="10"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_weight="4"/>
    <TextView
        android:id="@+id/TVsplitbar9"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_weight="0.1"
        android:background="@color/GREY_TEXT_LIGHT_SUPER">
    </TextView>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_weight="5"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:weightSum="100"
        android:orientation="horizontal"
        android:layout_weight="10">
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="7"/>
        <ImageView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="15"
            android:src="@drawable/audio_48_grey_light"
            android:id="@+id/IVaudioDownload"
            android:contentDescription="@string/audio" />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="5"/>
        <Button
            android:id="@+id/BTaudio"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="60"
            android:text="@string/audio"
            android:textAllCaps="false"
            android:background="@color/GREY_TEXT_LIGHT"
            android:textColor="@color/GREY_BACKGROUND_DARK_SUPER"
            android:textSize="18sp"
            android:textStyle="bold"
            />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="13"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_weight="2"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:weightSum="100"
        android:orientation="horizontal"
        android:layout_weight="10">
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="7"/>
        <ImageView
            android:id="@+id/IVvideo360Download"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="15"
            android:src="@drawable/video_48_grey_light"
            android:contentDescription="@string/video" />
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="5"/>
        <Button
            android:id="@+id/BTvideo360"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="60"
            android:text="@string/_360p"
            android:textAllCaps="false"
            android:background="@color/GREY_TEXT_LIGHT"
            android:textColor="@color/GREY_BACKGROUND_DARK_SUPER"
            android:textSize="18sp"
            android:textStyle="bold"/>
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="13"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_weight="2"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:weightSum="100"
        android:orientation="horizontal"
        android:layout_weight="10">
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="7"/>
        <ImageView
            android:id="@+id/IVvideo480Download"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="15"
            android:src="@drawable/video_48_grey_light"
            android:contentDescription="@string/video"/>
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="5"/>

        <Button
            android:id="@+id/BTvideo480"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="60"
            android:background="@color/GREY_TEXT_LIGHT"
            android:text="@string/_480p"
            android:textAllCaps="false"
            android:textColor="@color/GREY_BACKGROUND_DARK_SUPER"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="13"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_weight="2"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:weightSum="100"
        android:orientation="horizontal"
        android:layout_weight="10">
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="7"/>
        <ImageView
            android:id="@+id/IVvideo720Download"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="15"
            android:src="@drawable/video_48_grey_light"
            android:contentDescription="@string/video"/>
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="5"/>
        <Button
            android:id="@+id/BTvideo720"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="60"
            android:background="@color/GREY_TEXT_LIGHT"
            android:text="@string/_720p_hd"
            android:textAllCaps="false"
            android:textColor="@color/GREY_BACKGROUND_DARK_SUPER"
            android:textSize="18sp"
            android:textStyle="bold"/>

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="13"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_weight="2"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:weightSum="100"
        android:orientation="horizontal"
        android:layout_weight="10">
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="7"/>
        <ImageView
            android:id="@+id/IVvideo1080Download"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="15"
            android:src="@drawable/video_48_grey_light"
            android:contentDescription="@string/video"/>
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="5"/>
        <Button
            android:id="@+id/BTvideo1080"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="60"
            android:text="@string/_1080p_hd"
            android:textAllCaps="false"
            android:background="@color/GREY_TEXT_LIGHT"
            android:textColor="@color/GREY_BACKGROUND_DARK_SUPER"
            android:textSize="18sp"
            android:textStyle="bold"/>
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="13"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_weight="2"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:weightSum="100"
        android:orientation="horizontal"
        android:layout_weight="10">
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="7"/>
        <ImageView
            android:id="@+id/IVvideo2160Download"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="15"
            android:src="@drawable/video_48_grey_light"
            android:contentDescription="@string/video"/>
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="5"/>
        <Button
            android:id="@+id/BTvideo2160"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="60"
            android:text="@string/_2160p_4k_super_hd"
            android:textAllCaps="false"
            android:background="@color/GREY_TEXT_LIGHT"
            android:textColor="@color/GREY_BACKGROUND_DARK_SUPER"
            android:textSize="18sp"
            android:textStyle="bold"/>
        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_weight="13"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_weight="5"/>
    <TextView
        android:id="@+id/TVsplitbar10"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_weight="0.1"
        android:background="@color/GREY_TEXT_LIGHT_SUPER">
    </TextView>
</LinearLayout>
